{
    "name": "kennedyosaze/random-jokes",
    "description": "Create some random jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kennedy Osaze",
            "email": "me.osaze@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "KennedyOsaze\\RandomJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KennedyOsaze\\RandomJokes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KennedyOsaze\\RandomJokes\\RandomJokesServiceProvider"
            ],
            "aliases": {
                "RandomJokes": "KennedyOsaze\\RandomJokes\\Facades\\RandomJokes"
            }
        }
    }
}
